{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "declaration": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.vue", "src/**/*.ts", "src/**/*.tsx", "src/**/*.d.ts"],
  "exclude": ["node_modules", "es", "cjs"]
}
